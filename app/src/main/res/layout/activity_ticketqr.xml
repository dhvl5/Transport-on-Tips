<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".successfulfeed">

    <TextView
        android:id="@+id/label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/raleway_bold"
        android:text="Ticket Generated!"
        android:textSize="30sp"
        android:layout_marginTop="50dp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/label1"
        android:layout_marginLeft="40dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/raleway_bold"
        android:text="This is QR code of your E-Ticket.Scan this QR code for availing your Ticket."
        android:textSize="15sp"
        android:layout_marginTop="10dp"
        android:textColor="#000000"
        android:layout_below="@id/label" />

    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_below="@+id/label1"
        android:layout_marginTop="100dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/five" />

    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="550dp"
        android:text="next"
        android:background="@drawable/custom_btn1"
        android:textColor="#ffffff"
        android:fontFamily="@font/raleway_bold"
        android:onClick="addpic" />

</RelativeLayout>